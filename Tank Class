//Tank Class

import java.awt*;
import java.util*;

public class Tank 
{
  //variables
  private double xPos, yPos, xSpeed, ySpeed;
  private boolean up, down, right, left;
  private String myName;
  
  public Tank(String name)
  {
    myName = name;
  }
  public String getName()
  {
    return myName;  
  }
}
